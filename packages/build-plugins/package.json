{
  "name": "@splunk/olly-web-build-plugins",
  "private": true,
  "version": "0.19.1",
  "description": "Build plugins for users of @splunk/otel-web",
  "repository": "github:signalfx/splunk-otel-js-browser",
  "scripts": {
    "compile": "npm-run-all -s compile:*",
    "compile:tsc": "tsc --build tsconfig.cjs.json tsconfig.esm.json",
    "test:unit:ci": "env TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=tsconfig.cjs.json nyc mocha"
  },
  "author": "Splunk Observability Instrumentals Team <ssg-observability-instrumentals-npm@splunk.com>",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "types": "./dist/cjs/index.d.ts",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist/cjs/**/*.js",
    "dist/cjs/**/*.js.map",
    "dist/cjs/**/*.d.ts",
    "dist/esm/**/*.js",
    "dist/esm/**/*.js.map",
    "dist/esm/**/*.d.ts"
  ],
  "dependencies": {
    "axios": "^1.7.7",
    "form-data": "^4.0.1",
    "unplugin": "^1.14.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1"
  },
  "peerDependencies": {
    "webpack": "^4 || ^5"
  },
  "bugs": {
    "url": "https://github.com/signalfx/splunk-otel-js-browser/issues"
  },
  "homepage": "https://github.com/signalfx/splunk-otel-js-browser#readme"
}

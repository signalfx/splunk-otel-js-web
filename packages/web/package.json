{
	"name": "@splunk/otel-web",
	"version": "0.20.0-beta.3",
	"description": "Splunk distribution of Open Telemetry for browser environment.",
	"repository": "github:signalfx/splunk-otel-js-browser",
	"scripts": {
		"compile": "npm-run-all -s compile:*",
		"compile:browser": "rollup -c",
		"compile:tsc": "tsc --build tsconfig.cjs.json tsconfig.esm.json",
		"test:unit:watch": "karma start",
		"test:unit:ci": "nyc karma start --single-run --no-auto-watch --browsers ChromeHeadlessCI",
		"test:unit:ci-node": "env TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=tsconfig.cjs.json nyc mocha",
		"test:performance": "node ./performance-tests/runner.js"
	},
	"author": "Splunk Observability Instrumentals Team <ssg-observability-instrumentals-npm@splunk.com>",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/cjs/index.d.ts",
	"exports": {
		"types": "./dist/cjs/index.d.ts",
		"import": "./dist/esm/index.js",
		"require": "./dist/cjs/index.js"
	},
	"files": [
		"dist/cjs/**/*.js",
		"dist/cjs/**/*.js.map",
		"dist/cjs/**/*.d.ts",
		"dist/esm/**/*.js",
		"dist/esm/**/*.js.map",
		"dist/esm/**/*.d.ts"
	],
	"dependencies": {
		"@babel/runtime": "^7.24.7",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/core": "^1.25.1",
		"@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
		"@opentelemetry/exporter-zipkin": "^1.25.1",
		"@opentelemetry/instrumentation": "^0.52.1",
		"@opentelemetry/instrumentation-document-load": "^0.39.0",
		"@opentelemetry/instrumentation-fetch": "^0.52.1",
		"@opentelemetry/instrumentation-xml-http-request": "^0.52.1",
		"@opentelemetry/resources": "^1.25.1",
		"@opentelemetry/sdk-trace-base": "^1.25.1",
		"@opentelemetry/sdk-trace-web": "^1.25.1",
		"@opentelemetry/semantic-conventions": "^1.25.1",
		"shimmer": "^1.2.1",
		"web-vitals": "^3.5.2"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.24.7",
		"@babel/preset-env": "^7.24.7",
		"@babel/runtime-corejs3": "^7.24.7",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.3",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.1.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-typescript": "^11.1.2",
		"@types/chai": "^4.3.5",
		"@types/mocha": "^10.0.1",
		"@types/nightwatch": "^2.3.24",
		"@types/shimmer": "^1.0.2",
		"@types/sinon": "^17.0.3",
		"body-parser": "^1.20.2",
		"chai": "^4.3.7",
		"chrome-launcher": "^1.0.0",
		"chromedriver": "^131.0.1",
		"codecov": "^3.8.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"geckodriver": "^4.1.3",
		"generate-source-map": "0.0.5",
		"http-mitm-proxy": "^1.0.0",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage-istanbul-reporter": "^3.0.3",
		"karma-firefox-launcher": "^2.1.2",
		"karma-mocha": "^2.0.1",
		"karma-rollup-preprocessor": "^7.0.8",
		"karma-spec-reporter": "^0.0.36",
		"karma-websocket-server": "^1.0.0",
		"lighthouse": "^12.1.0",
		"markdownlint-cli": "^0.35.0",
		"mocha": "^10.2.0",
		"nightwatch": "^3.0.1",
		"node-fetch": "^3.3.2",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"puppeteer": "^22.11.2",
		"rollup": "^4.18.0",
		"rollup-plugin-istanbul": "^4.0.0",
		"rollup-plugin-polyfill-node": "^0.12.0",
		"sinon": "^15.2.0",
		"socket.io": "^4.7.1",
		"socket.io-client": "^4.7.1",
		"ts-node": "^10.9.1",
		"ws": "^8.17.1"
	},
	"bugs": {
		"url": "https://github.com/signalfx/splunk-otel-js-web/issues"
	},
	"homepage": "https://github.com/signalfx/splunk-otel-js-web#readme",
	"publishConfig": {
		"access": "public"
	}
}

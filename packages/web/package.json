{
	"name": "@splunk/otel-web",
	"version": "2.0.0",
	"description": "Splunk distribution of Open Telemetry for browser environment.",
	"keywords": [
		"splunk",
		"opentelemetry",
		"otel",
		"observability",
		"monitoring",
		"tracing",
		"browser",
		"web",
		"instrumentations",
		"telemetry"
	],
	"repository": "github:signalfx/splunk-otel-js-browser",
	"scripts": {
		"build": "npm-run-all -s build:*",
		"build:browser": "webpack --mode production --color && tsx scripts/remove-jaeger-vars.ts",
		"build:tsc": "tsc --build tsconfig.cjs.json tsconfig.esm.json",
		"dev": "npm-run-all -s dev:tsc:init -p dev:*",
		"dev:tsc:init": "tsc --build tsconfig.cjs.json tsconfig.esm.json",
		"dev:webpack": "webpack --mode development --watch --color",
		"dev:tsc": "tsc --build --watch tsconfig.cjs.json tsconfig.esm.json",
		"clean": "rm -rf dist .turbo",
		"wait": "wait-on dist/esm/index.js  dist/cjs/index.js"
	},
	"author": "Splunk Observability Instrumentals Team <ssg-observability-instrumentals-npm@splunk.com>",
	"license": "Apache-2.0",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/cjs/index.d.ts",
	"exports": {
		"types": "./dist/cjs/index.d.ts",
		"import": "./dist/esm/index.js",
		"require": "./dist/cjs/index.js"
	},
	"files": [
		"dist/cjs/**/*.js",
		"dist/cjs/**/*.js.map",
		"dist/cjs/**/*.d.ts",
		"dist/esm/**/*.js",
		"dist/esm/**/*.js.map",
		"dist/esm/**/*.d.ts"
	],
	"dependencies": {
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/core": "^1.25.1",
		"@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
		"@opentelemetry/exporter-zipkin": "^1.25.1",
		"@opentelemetry/instrumentation": "^0.52.1",
		"@opentelemetry/instrumentation-document-load": "^0.39.0",
		"@opentelemetry/instrumentation-fetch": "^0.52.1",
		"@opentelemetry/instrumentation-xml-http-request": "^0.52.1",
		"@opentelemetry/resources": "^1.25.1",
		"@opentelemetry/sdk-trace-base": "^1.25.1",
		"@opentelemetry/sdk-trace-web": "^1.25.1",
		"@opentelemetry/semantic-conventions": "^1.25.1",
		"core-js": "3.48.0",
		"shimmer": "^1.2.1",
		"web-vitals": "5.1.0"
	},
	"devDependencies": {
		"@swc/core": "^1.15.11",
		"@swc/helpers": "^0.5.18",
		"@types/shimmer": "^1.2.0",
		"fork-ts-checker-webpack-plugin": "^9.1.0",
		"npm-run-all": "^4.1.5",
		"socket.io-client": "^4.8.3",
		"source-map-loader": "^5.0.0",
		"swc-loader": "^0.2.7",
		"ts-loader": "^9.5.4",
		"wait-on": "^9.0.3",
		"webpack": "^5.104.1",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.3"
	},
	"bugs": {
		"url": "https://github.com/signalfx/splunk-otel-js-web/issues"
	},
	"homepage": "https://github.com/signalfx/splunk-otel-js-web#readme",
	"publishConfig": {
		"access": "public"
	}
}

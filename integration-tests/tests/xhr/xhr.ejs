<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>XHR</title>

	<%- renderAgent() %>
</head>
<body>
	<h1>XHR</h1>
	<script>
		function makeHXR(url) {
			var oReq = new XMLHttpRequest();
			oReq.open("GET", url);
			oReq.setRequestHeader('Content-Type', 'text/plain');
			oReq.send();

			return oReq;
		}

		makeHXR('/some-data');
	</script>
	</body>
</html>

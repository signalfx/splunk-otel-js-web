<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
  <title>Mouse events test</title>
  <%- renderAgent({
    instrumentations: {
      interactions: {
        events: {
          click: false,
          mousedown: false,
          mouseup: false
        }
      }
    }
  }) %>
</head>
<body>
  <h1>Mouse events test</h1>

  <button type="button" id="btn1">Button</button>
  <button type="button" id="btnGuard">Button</button>

  <pre id="scenarioDisplay"></pre>
  <script id="scenario">
    btn1.addEventListener('click', function () {});
    btn1.addEventListener('mousedown', function () {});
    btn1.addEventListener('mouseup', function () {});
    btnGuard.addEventListener('mouseup', function () {
      SplunkRum.provider.getTracer('default').startSpan('guard').end();
    });
  </script>
  <script>scenarioDisplay.innerHTML = scenario.innerHTML;</script>
</body>
</html>
